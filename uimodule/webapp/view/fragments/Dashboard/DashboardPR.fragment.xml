<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
	<SelectDialog
		title="Purchase Request"
		items="{oModel>/PurchaseRequest}"
		search="handleSearchPurchaseRequest"
		confirm="handleValueHelpClosePR">
		<StandardListItem
			title="{oModel>DocNum}"
			description="{oModel>RequestDate}" 
      info="{oModel>Status}"
      infoState="{oModel>InfoState}"/>
	</SelectDialog>
</core:FragmentDefinition>