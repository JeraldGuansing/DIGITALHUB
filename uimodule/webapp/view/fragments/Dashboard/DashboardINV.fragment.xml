<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
	<SelectDialog
		title="Inventory Request"
		items="{oModel>/InventoryRequest}"
		search="handleSearchINV"
		confirm="handleValueHelpCloseINV">
		<StandardListItem
			title="{oModel>DocNum}"
			description="{oModel>RequestDate}" 
      info="{oModel>Status}"
      infoState="{oModel>InfoState}"/>
	</SelectDialog>
</core:FragmentDefinition>