<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
	<SelectDialog
		title="Users"
		items="{oModel>/Users}"
		search="handleSearchUsers"
		confirm="handleValueCloseUsers">
		<StandardListItem
			title="{oModel>UserCode}"
			description="{oModel>UserName}" />
	</SelectDialog>
</core:FragmentDefinition>