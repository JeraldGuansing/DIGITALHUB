<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
	<SelectDialog
		title="Payment Request"
		items="{oModel>/PaymentRequestRecords}"
		search="handleSearchPM"
		confirm="handleValueClosePM">
		<StandardListItem
			title="{oModel>RequesterName}"
			description="{oModel>No}"
      info="{oModel>Status}" 
      infoState="{oModel>InfoState}"/>
	</SelectDialog>
</core:FragmentDefinition>