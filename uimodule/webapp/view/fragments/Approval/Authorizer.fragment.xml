<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
	<SelectDialog
		title="Users"
		items="{oModel>/Users}"
		search="handleSearchAuthorizer"
		confirm="handleValueCloseAuthorizer">
		<StandardListItem
			title="{oModel>UserName}"
			description="{oModel>UserCode}"/>
	</SelectDialog>
</core:FragmentDefinition>