<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
	<SelectDialog
		title="Documents"
		items="{oModel>/Documents}"
		search="handleSearchDocuments"
		confirm="handleValueCloseDocuments"
		cancel="handleValueCloseDocuments">
		<StandardListItem
			title="{oModel>Document}"
			description="{oModel>Document}" />
	</SelectDialog>
</core:FragmentDefinition>